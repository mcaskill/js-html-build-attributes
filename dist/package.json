{
    "name": "@mcaskill/html-build-attributes",
    "version": "0.2.4",
    "description": "Generate a string of HTML attributes.",
    "license": "MIT",
    "homepage": "https://github.com/mcaskill/js-html-build-attributes",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mcaskill/js-html-build-attributes.git"
    },
    "author": {
        "name": "Chauncey McAskill",
        "email": "chauncey@mcaskill.ca",
        "homepage": "https://github.com/mcaskill"
    },
    "keywords": [
        "front-end",
        "function",
        "html",
        "javascript",
        "templating"
    ],
    "type": "module",
    "types": "./esm/index.d.ts",
    "main": "./esm/index.js",
    "exports": {
        ".": {
            "import": {
                "types": "./esm/index.d.ts",
                "default": "./esm/index.js"
            },
            "require": {
                "types": "./cjs/index.d.cts",
                "default": "./cjs/index.cjs"
            }
        },
        "./*.js": {
            "import": {
                "types": "./esm/*.d.ts",
                "default": "./esm/*.js"
            },
            "require": {
                "types": "./cjs/*.d.cts",
                "default": "./cjs/*.cjs"
            }
        },
        "./attr": {
            "import": {
                "types": "./esm/attr/index.d.ts",
                "default": "./esm/attr/index.js"
            },
            "require": {
                "types": "./cjs/attr/index.d.cts",
                "default": "./cjs/attr/index.cjs"
            }
        },
        "./name": {
            "import": {
                "types": "./esm/name/index.d.ts",
                "default": "./esm/name/index.js"
            },
            "require": {
                "types": "./cjs/name/index.d.cts",
                "default": "./cjs/name/index.cjs"
            }
        },
        "./util": {
            "import": {
                "types": "./esm/util/index.d.ts",
                "default": "./esm/util/index.js"
            },
            "require": {
                "types": "./cjs/util/index.d.cts",
                "default": "./cjs/util/index.cjs"
            }
        },
        "./value": {
            "import": {
                "types": "./esm/value/index.d.ts",
                "default": "./esm/value/index.js"
            },
            "require": {
                "types": "./cjs/value/index.d.cts",
                "default": "./cjs/value/index.cjs"
            }
        }
    },
    "sideEffects": false,
    "engines": {
        "node": ">=16"
    }
}
